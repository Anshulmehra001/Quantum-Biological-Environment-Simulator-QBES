  "validation_criteria": {
    "description": "Criteria for QBES v1.2 self-certification",
    
    "accuracy_requirements": {
      "overall_accuracy": {
        "target": 0.98,
        "description": ">98% accuracy on literature benchmark suite"
      },
      "analytical_systems": {
        "target": 0.995,
        "description": ">99.5% accuracy on analytical benchmarks (exact solutions)"
      },
      "experimental_systems": {
        "target": 0.95,
        "description": ">95% agreement with experimental literature values"
      }
    },
    
    "numerical_stability": {
      "energy_conservation": {
        "tolerance": 1e-8,
        "description": "Energy should be conserved to numerical precision"
      },
      "unitarity_preservation": {
        "tolerance": 1e-10,
        "description": "Quantum evolution should preserve unitarity"
      },
      "density_matrix_validity": {
        "hermiticity_tolerance": 1e-12,
        "positivity_tolerance": 1e-14,
        "trace_tolerance": 1e-12,
        "description": "Density matrix must remain physical"
      }
    },
    
    "performance_requirements": {
      "small_systems": {
        "size": "N < 10 sites",
        "time_limit_seconds": 60,
        "memory_limit_MB": 500
      },
      "medium_systems": {
        "size": "10 <= N < 50 sites",
        "time_limit_seconds": 600,
        "memory_limit_MB": 2000
      },
      "large_systems": {
        "size": "N >= 50 sites",
        "time_limit_seconds": 3600,
        "memory_limit_MB": 8000
      }
    },
    
    "robustness_requirements": {
      "error_handling": "All errors should be caught with informative messages",
      "input_validation": "Invalid inputs should be rejected with clear guidance",
      "edge_cases": "Boundary conditions and edge cases should be handled gracefully",
      "reproducibility": "Results should be reproducible with same random seed"
    }
  },
  
  "usage_instructions": {
    "description": "How to use this reference data file",
    
    "validation_command": {
      "description": "Run full validation suite against literature",
      "command": "qbes validate --reference-data configs/reference_data.json --output validation_results/"
    },
    
    "benchmark_specific": {
      "description": "Run specific benchmark",
      "command": "qbes benchmark run --system fmo_complex_engel_2007 --reference configs/reference_data.json"
    },
    
    "accuracy_calculation": {
      "description": "Calculate accuracy metrics",
      "formula": "accuracy = 1 - mean(|simulated - reference| / reference)",
      "note": "Weighted average across all benchmarks"
    },
    
    "certification": {
      "description": "Certify QBES v1.2 compliance",
      "command": "qbes certify --reference configs/reference_data.json --report certification_report.pdf",
      "requirement": "Must pass all accuracy and stability criteria"
    }
  },
  
